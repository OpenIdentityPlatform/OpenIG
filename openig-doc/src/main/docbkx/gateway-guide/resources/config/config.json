{
    "heap": {
        "objects": [
            {
                "name": "LogSink",
                "type": "ConsoleLogSink",
                "config": {
                    "level": "DEBUG"
                }
            },
            {
                "name": "JwtSession",
                "type": "JwtSession"
            },
            {
                "name": "ClientHandler",
                "type": "ClientHandler"
            },
            {
                "name": "capture",
                "type": "CaptureDecorator",
                "config": {
                    "captureEntity": true
                }
            },
            {
                "name": "Router",
                "type": "Router",
                "capture": "all"
            }
        ]
    },
    "handler": "Router",
    "baseURI": "http://www.example.com:8081"
}
