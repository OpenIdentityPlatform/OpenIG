 <refmeta>
  <refentrytitle>NullLogSink</refentrytitle><manvolnum>5</manvolnum>
 </refmeta>

 <refnamediv>
  <refname>NullLogSink</refname>
  <refpurpose>discards log messages</refpurpose>
 </refnamediv>

 <refsect1>
  <title>Description</title>

  <indexterm>
   <primary>Miscellaneous Heap Objects</primary>
   <secondary>NullLogSink</secondary>
  </indexterm>

  <para>
   A log sink that discards all log messages.
  </para>
 </refsect1>

 <refsect1>
  <title>Usage</title>

  <programlisting language="javascript">
{
     "name": string,
     "type": "NullLogSink",
     "config": {}
}
  </programlisting>
 </refsect1>

 <refsect1>
  <title>Example</title>

  <programlisting language="javascript">
{
     "name": "LogSink",
     "type": "NullLogSink",
     "config": {}
}
  </programlisting>
 </refsect1>

